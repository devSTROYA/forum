{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/my-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/my-app/tsconfig.app.json"
  },
  "projects": {
    "app": {
      "type": "library",
      "root": "libs/app",
      "entryFile": "index",
      "sourceRoot": "libs/app/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "domain": {
      "type": "library",
      "root": "libs/domain",
      "entryFile": "index",
      "sourceRoot": "libs/domain/src",
      "compilerOptions": {
        "tsConfigPath": "libs/domain/tsconfig.lib.json"
      }
    },
    "dto": {
      "type": "library",
      "root": "libs/dto",
      "entryFile": "index",
      "sourceRoot": "libs/dto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dto/tsconfig.lib.json"
      }
    },
    "event": {
      "type": "library",
      "root": "libs/event",
      "entryFile": "index",
      "sourceRoot": "libs/event/src",
      "compilerOptions": {
        "tsConfigPath": "libs/event/tsconfig.lib.json"
      }
    },
    "external": {
      "type": "library",
      "root": "libs/external",
      "entryFile": "index",
      "sourceRoot": "libs/external/src",
      "compilerOptions": {
        "tsConfigPath": "libs/external/tsconfig.lib.json"
      }
    },
    "mapper": {
      "type": "library",
      "root": "libs/mapper",
      "entryFile": "index",
      "sourceRoot": "libs/mapper/src",
      "compilerOptions": {
        "tsConfigPath": "libs/mapper/tsconfig.lib.json"
      }
    },
    "repository": {
      "type": "library",
      "root": "libs/repository",
      "entryFile": "index",
      "sourceRoot": "libs/repository/src",
      "compilerOptions": {
        "tsConfigPath": "libs/repository/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/my-app"
}
